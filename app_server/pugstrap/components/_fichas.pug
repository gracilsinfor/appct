mixin par_turno(dados)
    .col-xs-12.col-sm-6.col-md-4.col-lg-3
        .panel.panel-primary
            .panel-heading
                h3.panel-title 
                    a(href='/Turno/'+ dados.id_turno) Turno: #{dados.id_turno}
            .panel-body
                .row
                    .col-xs-6.col-md-6.col-lg-6
                        .placeholder
                            .text-center 
                                a(href='/Condutor/'+ dados.id_condutor)
                                    h5 #{dados.nome_condutor}
                                    img.img-responsive(src=dados.imagem_condutor, alt=dados.nome_condutor)
                    .col-xs-6.col-md-6.col-lg-6
                        .placeholder 
                            .text-center
                                a(href='/Viatura/'+ dados.id_viatura)
                                    h5 #{dados.matricula}
                                    img.img-responsive(src=dados.imagem_viatura, alt=dados.matricula)
                .row
                    .col-xs-6.col-md-6.col-lg-6
                        .text-left
                            h6 Início:
                            span.text-muted.small #{dados.h_ini} : #{dados.d_ini}
                    .col-xs-6.col-md-6.col-lg-6
                        .text-left
                            h6 Fim:
                            span.text-muted.small #{dados.h_fim} : #{dados.d_fim} 

mixin card_viatura(dados)
    .col-xs-12.col-sm-6.col-md-4.col-lg-3
        .panel.panel-primary
            .panel-heading
                h4.panel-title 
                    a(href='/Viatura/'+ dados.id_viatura) Viatura: #{dados.id_viatura}
            .panel-body
                .row.placeholders
                    .col-xs-6.col-md6.col-lg-6
                        .placeholder
                            .text-center 
                                a(href=`/Viatura/${dados.id_viatura}`)
                                    h5 #{dados.matricula}
                                    img.img-responsive(src=dados.imagem_viatura, alt=dados.matricula)
                                    h5 #{dados.descricao_viatura}
                    .col-xs-6.col-md-6.col-lg-6
                        .placeholder 
                            h5 Kms: #{dados.odometro}
                            h5 Manutenções: 
                            if dados.kms_ultima_manutencao != null
                                h6 Última: #{dados.kms_ultima_manutencao} km
                                h6 Em: #{dados.data_ultima_manutencao}
                            else
                                h6 Última: n/d 
                            if dados.kms_proxima_manutencao != null
                                h6 Próxima: #{dados.kms_proxima_manutencao} km
                                h6 Em: #{dados.data_proxima_manutencao} 
                            else
                                h6 Próxima: n/d 
                            +yacheckbox-inline("chk_ev", "Elétrico", "ev", dados.ev, false)
                            +yacheckbox-inline("chk_ativa", "Ativa", "ativa", dados.ativa, false)

mixin card_condutor(dados)
    .col-xs-12.col-sm-6.col-md-4.col-lg-3
        .panel.panel-primary
            .panel-heading 
                h3.panel-title 
                    a(href='/Condutor/' + dados.id_condutor ) Condutor id: #{dados.id_condutor} 
            .panel-body
                .row
                    .col.placeholder
                        a(href='/Condutor/'+ dados.id_condutor)
                            .text-center 
                                h4.strong #{dados.nome_condutor}
                                p
                                    img.img-responsive(src=dados.imagem_condutor, alt=dados.nome_condutor)
                                p 
                                    +yacheckbox-inline("chk_condutor_ativo", "Ativo", "ativo", dados.ativo, false)
                                p 
                                p kms feitos: #{dados.kms_feitos}
                .row
                    .col
                        table.table-default(align="center")
                            tr 
                                td.text-left.strong Telefone: 
                                td.text-right #{dados.telefone_condutor}
                            tr
                                td.text-left.strong Email:
                                td.text-right.strong
                                    a(href='mailto:'+dados.email_condutor) #{dados.email_condutor}
                            tr
                                td.text-left.strong NIF:
                                td.text-right #{dados.nif_condutor}
                    
mixin ficha_abastecimento_mt(abastecimento)
    +panel-info("Ficha do Abastecimento combustível id: "+ abastecimento.id_abastecimento) 
        .row
            .col-xs-4.col-md-3.col-lg-2
                +yainput("text", "id_turno", "id_turno", "Turno:", "txt_turno", abastecimento.id_turno, "disabled")
            .col-xs-4.col-md-3.col-lg-2
                +yainput("text", "odometro", "odometro", "Odómetro:", "txt_odometro", abastecimento.odometro, "disabled")
            .col-xs-4.col-md-2.col-lg-2
                +yainput("date-time", "date_data", "data", "Data:", "data", abastecimento.data, "disabled")             
            .col-xs-4.col-md-3.col-lg-2
                +yainput("text", "litros", "litros", "Litros:", "txt_litros", abastecimento.litros, "disabled")
            .col-xs-4.col-md-3.col-lg-2
                +yainput("text", "custo", "custo", "Custo:", "txt_custo", abastecimento.custo, "disabled")
            .col-xs-4.col-md-2.col-lg-2
                span.align-middle
                    br
                    a.btn.btn-warning.btn-block(href='/Abastecimento/U/'+abastecimento.id_viatura+ "/" +abastecimento.id_abastecimento, role='button') Atualizar ficha do abastecimento »	

