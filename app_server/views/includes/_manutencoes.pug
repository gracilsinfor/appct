.container-fluid
	.row
		.col-sm-9.col-sm-offset-3.col-md-10.col-md-offset-2.main
			.row 
				.col-xs-12.col-md-6
					h2.page-header=title+" - "+sub_title
				.col-xs-12.col-md-6
					.pull-right
						+a-btn("/Manutencoes/C", "file-new", "Nova manutencao", "warning")  
			.row
				.col-xs-12.col-sm-6.col-md-3.col-lg-3
					+awsme_dropdown("ddmatriculas","dd_matriculas","filtro por viatura", matriculas, true, "car")
				.col-xs-12.col-sm-6.col-md-3.col-lg-3
					+awsme_dropdown("ddtipos","dd_tipos","filtro por tipo", tipos, true, "type")
				.col-xs-12.col-sm-6.col-md-3.col-lg-3
					+input_datetime("data_inicio", "in√≠cio ", Date.now(), "start")
				.col-xs-12.col-sm-6.col-md-3.col-lg-3
					+input_datetime("data_fim", "fim ", Date.now(), "end")
			.row
				.col-xs-12.col-sm-12.col-md-12.col-lg-12
					+lista_de_manutencoes(viaturas)

